<div class="govuk-summary-list__row">

  <dt class="govuk-summary-list__key govuk-!-width-one-third">
    <%= t('funding_application.check_answers.project.is_partnership.question') %>
  </dt>

  <dd class="govuk-summary-list__value">

    <% unless model_object.is_partnership.nil? %>

      <%=
        model_object.is_partnership ?
        t('funding_application.check_answers.project.is_partnership.answer_yes') :
        t('funding_application.check_answers.project.is_partnership.answer_no')
      %>

    <% else %>
    
      <%= t('generic.not_answered') %>

    <% end %>

  </dd>
  
</div>
