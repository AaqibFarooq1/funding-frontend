<div class="govuk-summary-list__row">

  <dt class="govuk-summary-list__key">
    <%= t('views.funding_application.gp_project.check_your_answers.project_end_date') %>
  </dt>

  <dd class="govuk-summary-list__value">
    <%=
      model_object.end_date.present? ?
      model_object.end_date.strftime('%d %b %Y').to_s :
      t('generic.not_answered')
    %>
  </dd>

  <%=
    render(
      partial: 'partials/funding_application/check_answers/change_link',
      locals: {
        link_url: change_link,
        link_hidden_label: t('views.funding_application.gp_project.check_your_answers.project_end_date')
      }
    )
  %>

</div>
