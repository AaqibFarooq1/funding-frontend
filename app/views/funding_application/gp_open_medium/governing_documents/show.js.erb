<% if @funding_application.open_medium.errors.any? %>

  document.title = "Error: <%== I18n.t('governing_document.page_title') %>";

  removeAllFormGroupErrors(
    "governing_document_file-form-group",
    "governing_document_file-errors"
  );

  createSummaryErrorsSkeleton();

  var errorKeys = [];

  <% @funding_application.open_medium.errors.each do |attr, msg| %>

      errorKeys.push("<%= attr.to_s %>");

      addSummaryError(
          false,
          "<%= attr %>",
          "<%= msg %>",
          "open_medium"
      );

      addFormGroupError(
          "<%= attr %>-form-group",
          "<%= attr %>-errors",
          "<%= attr %>",
          "<%= msg %>",
          "open_medium"
      );

  <% end %>

<% end %>
