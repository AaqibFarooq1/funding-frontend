<% if @funding_application.project.errors.any? %>

document.title = "Error: <%== I18n.t('governing_document.page_title') %>";

createSummaryErrorsSkeleton();

var errorKeys = [];

<% @funding_application.project.errors.each do |attr, msg| %>

addSummaryError(
    false,
    "<%= attr %>",
    "<%= msg %>",
    "project"
);

addFormGroupError(
    "governing-documents-form-group-main",
    "form-group-errors",
    "<%= attr %>",
    "<%= msg %>",
    "project"
);

<% end %>

removeFormGroupErrors(
    "project",
    ["governing_document_file"],
    errorKeys
);

<% end %>
